{
  "manifest_version": 3,
  "name": "MyLoc",
  "version": "1.0",
  "description": "Shows your IP location on a world map",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/positioning.png",
      "128": "images/positioning.png"
    }
  },
  "permissions": [
    "geolocation"
  ],
  "icons": {
    "48": "images/positioning.png",
    "128": "images/positioning.png"
  }
}
